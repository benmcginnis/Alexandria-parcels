{
  "name": "alexandria-parcels-map-viewer",
  "version": "1.0.0",
  "description": "Alexandria Parcels Map Viewer with React 19 + Mapbox",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p scripts/splitter/tsconfig.json",
    "build:vite": "vite build",
    "build:worker": "npx esbuild src/worker/index.ts --bundle --format=esm --outfile=dist/worker/index.js --external:@cloudflare/workers-types",
    "start": "node dist/run-splitter.js",
    "split": "npm run build && npm start",
    "dev": "ts-node scripts/splitter/run-splitter.ts",
    "dev:vite": "vite",
    "dev:worker": "wrangler dev --config wrangler.worker.toml --persist-to ./local-storage",
    "setup:local-dev": "./scripts/setup-local-dev.sh",
    "upload:local-data": "./scripts/upload-local-data.sh",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "npx playwright test --reporter=line",
    "test:e2e:headed": "npx playwright test --headed --reporter=line",
    "test:e2e:full": "./scripts/setup-and-test.sh",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "tsc --noEmit",
    "quality-check": "./scripts/quality-check.sh",
    "pre-commit": "npm run quality-check",
    "deploy": "wrangler deploy",
    "deploy:pages": "wrangler pages deploy dist --project-name alexandria-parcels",
    "validate:r2": "ts-node scripts/validate-r2-data.ts",
    "prepare": "husky",
    "node:use": "nvm use",
    "node:install": "nvm install",
    "node:check": "node --version && echo 'Expected: v24.x.x'",
    "node:validate": "node -e \"const v=process.version; if(!v.startsWith('v24')){console.error('❌ Wrong Node version:',v,'Expected v24.x.x'); process.exit(1);} console.log('✅ Node version OK:',v);\"",
    "preinstall": "npm run node:validate",
    "pretest": "npm run node:validate",
    "prebuild": "npm run node:validate"
  },
  "dependencies": {
    "@types/pako": "^2.0.4",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "pako": "^2.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rxdb": "^16.19.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.12.3",
    "@cloudflare/workers-types": "^4.20250905.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "wrangler": "^4.34.0"
  }
}
